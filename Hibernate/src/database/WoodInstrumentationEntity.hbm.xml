        <property name="flute">
            <column name="flute" sql-type="int(11)"/>
        </property>
        <property name="oboe">
            <column name="oboe" sql-type="int(11)"/>
        </property>
        <property name="clarinet">
            <column name="clarinet" sql-type="int(11)"/>
