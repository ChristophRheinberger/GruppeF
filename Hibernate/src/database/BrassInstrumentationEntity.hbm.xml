<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="database.BrassInstrumentationEntity" table="BrassInstrumentation" schema="sem4_team2">
        <id name="brassInstrumentationId">
            <column name="brassInstrumentationID" sql-type="int(11)"/>
        </id>
        <property name="horn">
            <column name="horn" sql-type="int(11)"/>
        </property>
        <property name="trumpet">
            <column name="trumpet" sql-type="int(11)"/>
        </property>
        <property name="trombone">
            <column name="trombone" sql-type="int(11)"/>
        </property>
        <property name="tube">
            <column name="tube" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>