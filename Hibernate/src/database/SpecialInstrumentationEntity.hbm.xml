<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="database.SpecialInstrumentationEntity" table="SpecialInstrumentation" schema="sem4_team2">
        <id name="specialInstrumentationId">
            <column name="specialInstrumentationID" sql-type="int(11)"/>
        </id>
        <property name="instrumentationId">
            <column name="instrumentationID" sql-type="int(11)"/>
        </property>
        <property name="specialInstrument">
            <column name="specialInstrument" sql-type="varchar(255)"/>
        </property>
        <property name="specialInstrumentationNumber">
            <column name="specialInstrumentationNumber" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>