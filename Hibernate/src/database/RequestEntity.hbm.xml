<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="database.RequestEntity" table="Request" schema="sem4_team2">
        <property name="eventDuty">
            <column name="eventDuty" sql-type="int(11)"/>
        </property>
        <property name="musician">
            <column name="musician" sql-type="int(11)"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>