<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="database.InstrumentTypeEntity" table="InstrumentType" schema="sem4_team2">
        <id name="instrumentTypeId">
            <column name="instrumentTypeID" sql-type="int(11)"/>
        </id>
        <property name="instrumentType">
            <column name="instrumentType" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>