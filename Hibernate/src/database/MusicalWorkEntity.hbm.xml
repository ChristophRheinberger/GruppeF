<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="database.MusicalWorkEntity" table="MusicalWork" schema="sem4_team2">
        <id name="musicalWorkId">
            <column name="musicalWorkID" sql-type="int(11)"/>
        </id>
        <property name="instrumentationId">
            <column name="instrumentationID" sql-type="int(11)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
        <property name="composer">
            <column name="composer" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>